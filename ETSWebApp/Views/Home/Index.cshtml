<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Apartman Gider Takip Sistemi</title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/css/prettyPhoto.css" rel="stylesheet">
    <link href="~/css/price-range.css" rel="stylesheet">
    <link href="~/css/animate.css" rel="stylesheet">
    <link href="~/css/main.css" rel="stylesheet">
    <link href="~/css/responsive.css" rel="stylesheet">


</head><!--/head-->

<body>

    <header id="header">
        @Html.Partial("_HeaderTop")
        @Html.Partial("_HeaderBottom")
    </header>

    <section id="form">
        <div class="container">
            <div class="row">
                <div class="col-sm-4 col-sm-offset-1">
                    <div class="login-form">
                        <h2>Ödeme Girmek İçin Tıklayın</h2>
                        <a asp-action="Odeme" type="submit" class="btn btn-default">Ödeme</a>
                        
                    </div>
                </div>
               
                <div class="col-sm-4">
                    <div class="signup-form">
                        <h2>Ödeme Kayıtlarını Görmek İçin Tıklayın</h2>
                       <a asp-action="Listeleme" type="submit" class="btn btn-default">Kayıtlar</a>
                    </div>
                </div>
            </div>
        </div>
    </section>


    @Html.Partial("_Footer")
    <script src="js/jquery.js"></script>
    <script src="js/price-range.js"></script>
    <script src="js/jquery.scrollUp.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.prettyPhoto.js"></script>
    <script src="js/main.js"></script>
</body>
</html>